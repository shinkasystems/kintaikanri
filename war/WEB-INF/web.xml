<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<filter>
		<filter-name>wickletFilter</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter
		</filter-class>
		<init-param>
			<param-name>applicationClassName</param-name>
			<param-value>net.shinkasystems.kintai.KintaiWebApplication</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>wickletFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>